<view class="warp"><mt-button vue-id="1" type="primary" bind:__l="__l" vue-slots="{{['default']}}">mint-ui</mt-button><view class="input"><input type="text" placeholder="输入的二维码数" data-event-opts="{{[['input',[['number',['$event']]]]]}}" value="{{value}}" bindinput="__e"/><button type="primary" size="mini" data-event-opts="{{[['tap',[['search',['$event']]]]]}}" bindtap="__e">搜索</button></view><view class="box"><t-table vue-id="2" border="2" border-color="#95b99e" is-check="{{true}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{tableList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><t-tr vue-id="{{('3-'+index)+','+('2')}}" font-size="12" color="#5d6f61" align="right" bind:__l="__l" vue-slots="{{['default']}}"><t-td vue-id="{{('4-'+index)+','+('3-'+index)}}" align="left" bind:__l="__l" vue-slots="{{['default']}}">{{item.name}}</t-td></t-tr></block></t-table></view></view>